"""
"""

def addEdge(adj, u, v): 
    adj[u-1].append(v-1)
    adj[v-1].append(u-1)

def allconnected(adj):
    s=[]
    for i in range(len(adj)):
        l=[j for j in range(len(adj))if j!=i]
        if l==sorted(adj[i]):
            s.append(str(i+1))
    if len(s)==0:
        s=["-1"]
    return " ".join(s)
  
v=int(input())
u=int(input())
adj = [[] for i in range(v)]
for i in range(u):
    x,y=map(int,input().split())
    addEdge(adj,x,y)
print(allconnected(adj) if u>=v-1 else "-1")
